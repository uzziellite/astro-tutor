---
import Layout from "@layouts/layout.astro"
import SideMenu from "@components/SideMenu.svelte"

// src/pages/assesment/subjects/[subject]/[topic].astro
export async function getStaticPaths() {
  
  const levels = [
  	{
  		"subject":"pre-k/compare-by-matching"
  	},
  ]

  return levels.map(({ subject }) => {
    return {
      params: { subject }
    }
  })
}

const {subject} = Astro.params

const title = `${subject.charAt(0).toUpperCase()}${subject.substr(1)}`
---

<Layout title={title} description="">
  <div class="container mx-auto p-4 mt-[70px]"></div>
  <SideMenu client:load />
</Layout>
